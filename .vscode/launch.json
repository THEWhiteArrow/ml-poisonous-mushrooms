{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Pytest",
			"type": "debugpy",
			"request": "launch",
			"module": "pytest",
			"args": ["-v", "-s", "${workspaceFolder}"],
			"presentation": {
				"hidden": false,
				"group": "test",
				"order": 1
			},
			"console": "internalConsole"
		},
		{
			"name": "Pytest Mark Dev",
			"type": "debugpy",
			"request": "launch",
			"module": "pytest",
			"args": ["-v", "-s", "-m", "dev", "${workspaceFolder}"],
			"presentation": {
				"hidden": false,
				"group": "test",
				"order": 1
			},
			"console": "internalConsole"
		},
		{
			"name": "analysis_hyper.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/analysis/analysis_hyper.py",
			"console": "internalConsole"
		},
		{
			"name": "debug.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/debug.py",
			"console": "internalConsole"
		},
		{
			"name": "ensemble_task.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/ensemble_task.py",
			"console": "internalConsole"
		},
		{
			"name": "ensemble_task2.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/ensemble_task2.py",
			"console": "internalConsole"
		},
		{
			"name": "hyper_task.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/hyper_task.py",
			"console": "internalConsole"
		},
		{
			"name": "prediction_task.py",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/ml_poisonous_mushrooms/prediction_task.py",
			"console": "internalConsole"
		}
	]
}
